  object JudgeQuery: TQuery
    SQL.Strings = (
      'SELECT  A.CardNo ,'
      '              B.CardCode ,'
      '              B.DelFlag'
      'FROM    dbo.PatientBase A ( NOLOCK )'
      
        '        LEFT JOIN dbo.CardforPatient B ( NOLOCK ) ON A.PatientID' +
        ' = B.PatientID'
      'WHERE   PersonID = :PID;')
    Left = 240
    Top = 8
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'PID'
        ParamType = ptUnknown
