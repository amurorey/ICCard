  object SearchQuery: TQuery
    SQL.Strings = (
      'select'
      '  Type,'
      '  InvoiceID,'
      '  CZNum,'
      '  BegNum,'
      '  EndNum,'
      
        '  SubString('#39'0000000000'#39',0,FPLength-LEN(CurNum)+1)+convert(varch' +
        'ar(10),CurNum) as CurNum,'
      '  ParentInvoiceID,'
      '  FPLength,'
      '  FPCount'
      '  from'
      '(select'
      '  case when Type=0 then '#39#38376#35786#21457#31080#39
      '       when Type=1 then '#39#20303#38498#21457#31080#39
      '       when Type=2 then '#39#39044#20132#37329#25910#25454#39
      '  end as Type,'
      '  a.InvoiceID,'
      '  a.CZNum,'
      
        '  SubString('#39'0000000000'#39',0,FPLength-LEN(a.BegNum)+1)+convert(var' +
        'char(10),a.BegNum) as BegNum,'
      
        '  SubString('#39'0000000000'#39',0,FPLength-LEN(a.EndNum)+1)+convert(var' +
        'char(10),a.EndNum) as EndNum,'
      '  case when a.CurNum is null and b.CurNum is null then null'
      
        '       when a.CurNum is not null and b.CurNum is null then a.Cur' +
        'Num'
      '       when b.CurNum is not null then b.CurNum'
      '  end as CurNum,'
      '  a.ParentInvoiceID,'
      '  a.FPLength,'
      
        '  a.EndNum-isnull(case when a.CurNum is null and b.CurNum is nul' +
        'l then null'
      
        '                       when a.CurNum is not null and b.CurNum is' +
        ' null then a.CurNum'
      '                       when b.CurNum is not null then b.CurNum'
      '                  end+1,a.BegNum)+1 as FPCount,'
      '  a.Ver'
      ''
      ' from Invoice_Store a(nolock)'
      ' left join'
      
        ' (select InvoiceID,Max(EndNum) as CurNum from Invoice_Temp (nolo' +
        'ck) where UserID=:UserID group by InvoiceID) b'
      ' on a.InvoiceID=b.InvoiceID'
      ' where (a.EndNum-isnull(a.CurNum,a.BegNum-1)>0) and'
      '       (a.EndNum-isnull(b.CurNum,a.BegNum-1)>0) and'
      '  (a.InvoiceID like :FilterText or'
      '  a.Type like case when :FilterText like '#39'%'#38376#35786'%'#39' then 0'
      '                 when :FilterText like '#39'%'#20303#38498'%'#39' then 1 end or'
      '  a.CZNum like :FilterText or'
      '  a.OperName like :FilterText)) a'
      'order by'
      '  a.Type,a.Ver,a.InvoiceID'
      ''
      ' '
      ' '
      ' '
      ' '
      ' '
      ' '
      ' '
      ' ')
